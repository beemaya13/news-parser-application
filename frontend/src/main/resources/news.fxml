<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nilga.newsclient.controller.NewsController">
    <Label fx:id="headlineLabel" text="Headline" />
    <Label fx:id="descriptionLabel" text="Description" />
    <Label fx:id="publicationTimeLabel" text="Publication Time" />
    <Button text="Previous" onAction="#previousNews" />
    <Button text="Next" onAction="#nextNews" />
</VBox>
